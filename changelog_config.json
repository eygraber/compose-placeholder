{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature"]
    },
    {
      "title": "## âš™ï¸ Chores",
      "labels": ["chore"]
    },
    {
      "title": "## ğŸ› Bugs",
      "labels": ["bug"]
    },
    {
      "title": "## ğŸ§ª Tests",
      "labels": ["test"]
    },
    {
      "title": "## \uD83D\uDCD6 Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "\uD83D\uDCE6 Dependencies",
      "labels": ["dependencies"]
    }
  ],
  "ignore_labels": [
    "duplicate", "good first issue", "help wanted", "invalid", "question", "wontfix", "skip release notes", "hold"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1"
    },
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "on_property": "title"
    }
  ],
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "- $4\n  - $6"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "tag_resolver": {
    "method": "semver"
  },
  "base_branches": [
    "master"
  ]
}
